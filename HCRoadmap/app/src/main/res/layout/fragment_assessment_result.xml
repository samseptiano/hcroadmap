<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:fillViewport="true"
    card_view:layout_behavior="@string/appbar_scrolling_view_behavior"

    tools:context=".Fragment.TalentMapping.SubTalentProfile.AssessmentResultFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/colorPrimary"
            android:textSize="12sp"
            android:textStyle="bold"
            android:gravity="left"
            android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
            android:layout_marginLeft="15dp"
            android:text="Assessment Result" />
    <LinearLayout
        android:id="@+id/lnDataDiri"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:gravity="right"
                android:layout_marginRight="5dp"
                android:text="Tanggal Assessment:" />
            <TextView
                android:id="@+id/tvtanggalAssessment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textSize="12sp"
                android:layout_marginRight="5dp"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:gravity="right"
                android:layout_marginRight="5dp"
                android:text="PIC Assessment:" />
            <TextView
                android:id="@+id/tvPICAssessment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textSize="12sp"
                android:layout_marginRight="5dp"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:gravity="right"
                android:layout_marginRight="5dp"
                android:text="Assessment Score:" />
            <TextView
                android:id="@+id/tvAssessmentScore"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textSize="12sp"
                android:layout_marginRight="5dp"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:gravity="right"
                android:layout_marginRight="5dp"
                android:text="Assessment Capacity" />
            <TextView
                android:id="@+id/tvAssessmentCapacity"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textSize="12sp"
                android:layout_marginRight="5dp"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:gravity="right"
                android:layout_marginRight="5dp"
                android:text="Assessment Result:" />
            <TextView
                android:id="@+id/tvAssessmentResult"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textSize="12sp"
                android:layout_marginRight="5dp"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:text="" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:textSize="12sp"
                android:gravity="right"
                android:layout_marginRight="5dp"
                android:text="Profil DISC:" />
            <TextView
                android:id="@+id/tvDISC"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:textSize="12sp"
                android:layout_marginRight="5dp"
                android:textColor="@color/black"
                android:layout_weight="1"
                android:text="" />
        </LinearLayout>


        <com.github.chrisbanes.photoview.PhotoView
            android:id="@+id/imgAssessment1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scaleType="centerInside"
            android:layout_margin="10dp"
            android:src="@drawable/imgalt" />

        <com.github.chrisbanes.photoview.PhotoView
            android:id="@+id/imgAssessment2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scaleType="centerInside"
            android:layout_margin="10dp"
            android:src="@drawable/imgalt" />
        <com.github.chrisbanes.photoview.PhotoView
            android:id="@+id/imgAssessment3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scaleType="centerInside"
            android:layout_margin="10dp"
            android:src="@drawable/imgalt" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="5dp"
                android:background="@drawable/shape_black_border"
                android:layout_weight="1">
        <androidx.cardview.widget.CardView
            android:id="@+id/cvStrength"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            card_view:cardBackgroundColor="@color/colorPrimary"
            card_view:cardElevation="0dp"
            android:layout_marginBottom="0dp"
            android:paddingBottom="0dp"
            card_view:cardCornerRadius="0dp"
            android:paddingTop="0dp"
            android:outlineSpotShadowColor="@color/black"
            card_view:cardUseCompatPadding="true"
            ><TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:textSize="12sp"
            android:textStyle="bold"
            android:gravity="left"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:text=" Strength" />
            <!--                android:layout_height="20dp"-->
            <!--                android:layout_margin="8dp"-->
            <!--                android:scaleType="fitXY"-->
            <!--                android:layout_gravity="right"-->
            <!--                android:background="@color/transparent"-->
            <!--                android:src="@drawable/expand_up">-->
            <!--            </androidx.appcompat.widget.AppCompatImageButton>-->
        </androidx.cardview.widget.CardView>
        <TextView
            android:id="@+id/tvStrength"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/black"
            android:textSize="11sp"
            android:gravity="left"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:layout_marginLeft="15dp"
            android:text="Strength" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="5dp"
                android:background="@drawable/shape_black_border"
                android:layout_weight="1">
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvAreaImprovement"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    card_view:cardBackgroundColor="@color/colorPrimary"
                    card_view:cardElevation="0dp"
                    android:layout_marginBottom="0dp"
                    android:paddingBottom="0dp"
                    card_view:cardCornerRadius="0dp"
                    android:paddingTop="0dp"
                    android:outlineSpotShadowColor="@color/black"
                    card_view:cardUseCompatPadding="true"
                    ><TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    android:gravity="left"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:text=" Area Improvement" />
                    <!--                android:layout_height="20dp"-->
                    <!--                android:layout_margin="8dp"-->
                    <!--                android:scaleType="fitXY"-->
                    <!--                android:layout_gravity="right"-->
                    <!--                android:background="@color/transparent"-->
                    <!--                android:src="@drawable/expand_up">-->
                    <!--            </androidx.appcompat.widget.AppCompatImageButton>-->
                </androidx.cardview.widget.CardView>
                <TextView
                    android:id="@+id/tvAreaImprovement"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:gravity="left"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp"
                    android:layout_marginLeft="15dp"
                    android:text="Area Improvement" />
            </LinearLayout>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="20dp"
            android:background="@color/black"/>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/colorPrimary"
            android:textSize="14sp"
            android:textStyle="bold"
            android:gravity="left"
            android:layout_margin="15dp"
            android:text="Human Asset Value" />

        <com.github.chrisbanes.photoview.PhotoView
            android:id="@+id/imgAssessment4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:scaleType="centerInside"
            android:layout_margin="10dp"
            android:src="@drawable/imgalt" />

    </LinearLayout>
    </LinearLayout>


</androidx.core.widget.NestedScrollView>