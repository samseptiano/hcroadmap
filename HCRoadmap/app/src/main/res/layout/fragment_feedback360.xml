<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:fillViewport="true"
    card_view:layout_behavior="@string/appbar_scrolling_view_behavior"

    tools:context=".Fragment.TalentMapping.SubTalentProfile.feedback360Fragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/colorPrimary"
            android:layout_marginLeft="15dp"
            android:textStyle="bold"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="5dp"
            android:textSize="12sp"

            android:gravity="left"
            android:text="360 Feedback" />

        <TextView
            android:id="@+id/tvTanggal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="25dp"
            android:textSize="11sp"
            android:gravity="left"
            android:text="" />
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/shape_black_border"
        android:orientation="vertical">
        <androidx.cardview.widget.CardView
            android:id="@+id/cvStrength"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            card_view:cardBackgroundColor="@color/greenPastel"
            card_view:cardElevation="0dp"
            android:layout_marginBottom="0dp"
            android:paddingBottom="0dp"
            card_view:cardCornerRadius="0dp"
            android:paddingTop="15dp"
            android:outlineSpotShadowColor="@color/black"
            card_view:cardUseCompatPadding="true"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/colorPrimary"
                android:textSize="12sp"
                android:textStyle="bold"
                android:gravity="left"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="15dp"
                android:text="Strength" />
        </androidx.cardview.widget.CardView>
        <LinearLayout
            android:id="@+id/lnStrength"
            android:layout_width="match_parent"
            android:layout_marginRight="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="30dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">

                <TextView
                    android:id="@+id/tvStrength"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/black"
                    android:textSize="11sp"
                    android:gravity="left"
                    android:text="Strength" />
            </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/shape_black_border"
        android:orientation="vertical">
        <androidx.cardview.widget.CardView
            android:id="@+id/cvAreaImprovement"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            card_view:cardBackgroundColor="@color/pinkPastel"
            card_view:cardElevation="0dp"
            android:layout_marginBottom="0dp"
            android:paddingBottom="0dp"
            card_view:cardCornerRadius="0dp"
            android:paddingTop="15dp"
            android:outlineSpotShadowColor="@color/black"
            card_view:cardUseCompatPadding="true"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/colorPrimary"
                android:textSize="12sp"
                android:textStyle="bold"
                android:gravity="left"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="15dp"
                android:text="Area Improvement" />
        </androidx.cardview.widget.CardView>
        <LinearLayout
            android:id="@+id/lnAreaImprovement"
            android:layout_width="match_parent"
            android:layout_marginRight="15dp"
            android:layout_marginLeft="15dp"
            android:layout_marginBottom="30dp"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tvAreaImprovement"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:textSize="11sp"
                android:gravity="left"
                android:text="Area improvement" />
        </LinearLayout>
    </LinearLayout></LinearLayout>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>